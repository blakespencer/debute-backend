              "path": ["analytics", "order-count"]
            }
          },
          "response": []
        },
        {
          "name": "Get Order Count (Date Range)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/analytics/order-count?start=2024-01-01&end=2024-12-31",
              "host": ["{{baseUrl}}"],
              "path": ["analytics", "order-count"],
              "query": [
                {
                  "key": "start",
                  "value": "2024-01-01"
                },
                {
                  "key": "end",
                  "value": "2024-12-31"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Get Average Order Value",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/analytics/average-order-value",
              "host": ["{{baseUrl}}"],
              "path": ["analytics", "average-order-value"]
            }
          },
          "response": []
        },
        {
          "name": "Get Average Order Value (Date Range)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/analytics/average-order-value?start=2024-01-01&end=2024-12-31",
              "host": ["{{baseUrl}}"],
              "path": ["analytics", "average-order-value"],
              "query": [
                {
                  "key": "start",
                  "value": "2024-01-01"
                },
                {
                  "key": "end",
                  "value": "2024-12-31"
                }
              ]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "Shopify",
      "description": "Shopify API - Backend handles all Shopify credentials internally",
      "item": [
        {
          "name": "Test Shopify Connection",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/shopify/test",
              "host": ["{{baseUrl}}"],
              "path": ["shopify", "test"]
            }
          },
          "response": []
        },
        {
          "name": "Sync Orders",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"limit\": 50\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/shopify/sync",
              "host": ["{{baseUrl}}"],
              "path": ["shopify", "sync"]
            }
          },
          "response": []
        },
        {
          "name": "Sync Orders Since Date",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"fromDate\": \"2024-01-01T00:00:00Z\",\n  \"limit\": 100\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/shopify/sync",
              "host": ["{{baseUrl}}"],
              "path": ["shopify", "sync"]
            }
          },
          "response": []
        },
        {
          "name": "Get Synced Orders",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/shopify/orders?limit=20&offset=0",
              "host": ["{{baseUrl}}"],
              "path": ["shopify", "orders"],
              "query": [
                {
                  "key": "limit",
                  "value": "20"
                },
                {
                  "key": "offset",
                  "value": "0"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Get Synced Orders (Page 2)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/shopify/orders?limit=20&offset=20",
              "host": ["{{baseUrl}}"],
              "path": ["shopify", "orders"],
              "query": [
                {
                  "key": "store",
                  "value": "{{shopifyStoreDomain}}"
                },
                {
                  "key": "limit",
                  "value": "20"
                },
                {
                  "key": "offset",
                  "value": "20"
                }
              ]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "SWAP Returns",
      "description": "SWAP Returns API - Backend handles all SWAP credentials internally",
      "item": [
        {
          "name": "Test SWAP Connection",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/swap/test",
              "host": ["{{baseUrl}}"],
              "path": ["swap", "test"]
            }
          },
          "response": []
        },
        {
          "name": "Sync Returns",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"limit\": 10\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/swap/sync",
              "host": ["{{baseUrl}}"],
              "path": ["swap", "sync"]
            }
          },
          "response": []
        },
        {
          "name": "Sync Returns Since Date",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "x-api-key",
                "value": "{{swapApiKey}}",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"fromDate\": \"2024-01-01T00:00:00Z\",\n  \"limit\": 20\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/swap/sync",
              "host": ["{{baseUrl}}"],
              "path": ["swap", "sync"]
            }
          },
          "response": []
        },
        {
          "name": "Get Returns",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/swap/returns?limit=20&offset=0",
              "host": ["{{baseUrl}}"],
              "path": ["swap", "returns"],
              "query": [
                {
                  "key": "limit",
                  "value": "20"
                },
                {
                  "key": "offset",
                  "value": "0"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Get Returns by Status",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/swap/returns?status=Closed&limit=10",
              "host": ["{{baseUrl}}"],
              "path": ["swap", "returns"],
              "query": [
                {
                  "key": "status",
                  "value": "Closed"
                },
                {
                  "key": "limit",
                  "value": "10"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Get Return by ID",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/swap/returns/:id",
              "host": ["{{baseUrl}}"],
              "path": ["swap", "returns", ":id"],
              "query": [
                {
                  "key": "store",
                  "value": "{{swapStoreId}}"
                }
              ],
              "variable": [
                {
                  "key": "id",
                  "value": "replace-with-actual-return-id"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Get Total Refunds",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/swap/analytics/total-refunds",
