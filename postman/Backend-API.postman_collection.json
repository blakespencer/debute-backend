{
  "info": {
    "name": "Debute Backend API",
    "description": "Collection for testing the Debute Backend API with Analytics and Shopify endpoints",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:3000",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Health Check",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/health",
          "host": ["{{baseUrl}}"],
          "path": ["health"]
        }
      },
      "response": []
    },
    {
      "name": "Analytics",
      "item": [
        {
          "name": "Get Total Revenue",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/analytics/total-revenue",
              "host": ["{{baseUrl}}"],
              "path": ["analytics", "total-revenue"]
            }
          },
          "response": []
        },
        {
          "name": "Get Total Revenue (Date Range)",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{baseUrl}}/analytics/total-revenue?start=2024-01-01&end=2024-12-31",
              "host": ["{{baseUrl}}"],
              "path": ["analytics", "total-revenue"],
              "query": [
                {
                  "key": "start",
                  "value": "2024-01-01"
                },
                {
                  "key": "end",
                  "value": "2024-12-31"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Get Order Count",
          "request": {
            "method": "GET",
